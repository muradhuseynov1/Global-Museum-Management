{
  "name": "museum-management-app",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd node-server && node server.mjs",
    "client": "cd museum-app1 && npm start",
    "lint": "eslint 'node-server/**/*.js' 'museum-app1/**/*.js'",
    "format": "prettier --write 'node-server/**/*.js' 'museum-app1/**/*.js'"
  },
  "devDependencies": {
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/lab": "^5.0.0-alpha.167",
    "@mui/material": "^5.15.15",
    "@mui/system": "^5.15.15",
    "@mui/x-data-grid": "^7.1.0",
    "@mui/x-date-pickers": "^6.19.6",
    "country-list": "^2.3.0",
    "currency-codes": "^2.1.0",
